              </nav>
              </body>
              <div class="container mt-5">
                    <div class="row">
                        <div class="col col-lg-6">
                            
                                <table class="table">
                                    <thead class="table-dark">
                                        <tr>
                                            <th>Name</th>
                                            <th>Address</th>
                                            <th>Gender</th>
                                            <th>D.o.B</th>
                                            <th>email</th>
                                            <th>username</th>
                                            <th>password</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for(var i=0;i<data.length;i++){ %>
                                            <tr>
                                                <td><%= data[i].n %></td>
                                                <td><%= data[i].a %></td>
                                                <td><%= data[i].gender %></td>
                                                <td><%=data[i].dob%></td>
                                                <td><%=data[i].email%></td>
                                                <td><%=data[i].uname%></td>
                                                <td><%=data[i].upass%></td>

                                            </tr>
                                        <% } %>
                                    </tbody>
                                </table>
                            
                        </div>
                    </div>
                </div>
            </body>
